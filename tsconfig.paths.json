{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": false,
    "baseUrl": ".",
    "paths": {
      "@/": ["src/"],
      "@/assets": ["src/assets"],
      "@/assets/*": ["src/assets/*"],
      "@/components": ["src/components"],
      "@/components/*": ["src/components/*"],
      "@/containers": ["src/containers"],
      "@/containers/*": ["src/containers/*"],
      "@/hooks": ["src/hooks"],
      "@/hooks/*": ["src/hooks/*"],
      "@/pages": ["src/pages"],
      "@/pages/*": ["src/pages/*"],
      "@/store": ["src/store"],
      "@/store/*": ["src/store/*"],
      "@/config": ["src/config"],
      "@/config/*": ["src/config/*"]
    }
  }
}
